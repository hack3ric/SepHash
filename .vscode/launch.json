{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/ser_cli",
            "args": [
              "--server_ip", "10.77.110.160" ,
              "--num_machine" , "3" ,
              "--num_cli" , "8",
              "--num_coro" , "1",
              "--gid_idx" , "1",
              "--max_coro" , "256",
              "--cq_size" , "64",
              "--machine_id" , "0",
              "--load_num" , "10000000",
              "--num_op" , "10000000",
              "--pattern_type" , "2",
              "--insert_frac" , "0.5",
              "--read_frac" , "0.5",
              "--update_frac" , "0.0",
              "--delete_frac" , "0.0",
              "--read_size" , "64"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            // "preLaunchTask": "build",
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ]
}